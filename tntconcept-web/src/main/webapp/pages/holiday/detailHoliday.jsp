<%@page language="java" contentType="text/html; charset=UTF-8"%>

<%@include file="/inc/tlibs.jsp" %>

<html>
  <head>
    <%@include file="/inc/uiCore.jsp" %>
  </head>	
  <body>
    
    <!-- editHoliday.jsp: generated by stajanov code generator -->
    <f:loadBundle basename="com.autentia.tnt.resources.messages" var="msg" />
    <i:location place="detailHoliday" msg="${msg}"/> 
    
    <f:view>
        <%@include file="/inc/header.jsp" %>
      <h:form id="holiday" enctype="multipart/form-data">
        
        <%-- Header --%>
        <i:titleBar name="${holidayBean.description}" msg="${msg}">
          <t:commandLink action="#{holidayBean.edit}" immediate="true" rendered="#{holidayBean.editAvailable}">
            <f:param name="rowid" value="#{holidayBean.id}" />
            <h:graphicImage title="#{msg.entityActions_edit}"  value="/img/edit.gif" styleClass="cmdImg" />
          </t:commandLink>
          <h:commandLink action="#{holidayBean.delete}" rendered="#{holidayBean.deleteAvailable}" onclick="if( !confirm('#{msg['question.confirmDelete']}') ) return false;">
            <h:graphicImage title="#{msg.entityActions_delete}"  value="/img/delete.gif" styleClass="titleImg" />
          </h:commandLink>
          <h:commandLink action="#{holidayBean.list}" immediate="true">
            <h:graphicImage title="#{msg.entityActions_back}"  value="/img/back.gif" styleClass="titleImg" />
          </h:commandLink>
        </i:titleBar>


        <%-- Detail form --%>
        <table class="detailTable" cellpadding="0" cellspacing="0">

            
  
      <%-- Ignored field: id --%>
  
                            
  
      <%-- Field: description --%>
    <tr>
	<td class="detailLabelRW">${msg['holiday.description']}:</td>
	<td class="detailFieldCell">
         	       <h:outputText value="#{holidayBean.description}" />
 	      </td>
    </tr>
                            
  
      <%-- Field: date --%>
    <tr>
	<td class="detailLabelRW">${msg['holiday.date']}:</td>
	<td class="detailFieldCell">
         	    	<h:outputText value="#{holidayBean.date}" converter="autentia.dateConverter" />
 	          </td>
    </tr>
                            
  
      <%-- Ignored field: ownerId --%>
  
                            
  
      <%-- Ignored field: departmentId --%>
  
                            
  
      <%-- Ignored field: insertDate --%>
  
                            
  
      <%-- Ignored field: updateDate --%>
  
                                                  
        </table>

      </h:form>
    </f:view>
    
  </body>
</html>  		
<%-- holiday - generated by stajanov (do not edit/delete) --%>
