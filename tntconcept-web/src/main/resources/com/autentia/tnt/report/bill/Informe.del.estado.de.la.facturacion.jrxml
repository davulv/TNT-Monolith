	<queryString><![CDATA[select tabla.mes,COALESCE(sumaEmitidas,0) as sumaEmitidas,tablas.mes,COALESCE(sumaPagadas,0) as sumaPagadas
from (SELECT creationDate,concat(quarter(b.creationDate),' / ',year(b.creationDate)) as mes	,COALESCE(sum(bb.amount*bb.units*(1 + (bb.iva/100))),0) as sumaEmitidas FROM Bill b,BillBreakDown bb where bb.billId=b.id and b.billType='issued' and b.state!='IN_CREDITTITLE' group by mes) as tabla
      left join
     (SELECT concat(quarter(b.creationDate),' / ',year(b.creationDate)) as mes	,COALESCE(sum(bb.amount*bb.units*(1 + (bb.iva/100))),0) as sumaPagadas FROM Bill b,BillBreakDown bb where bb.billId=b.id and state='paid' and b.billType='issued' group by mes) as tablas
     on tabla.mes = tablas.mes order by year(tabla.creationDate),quarter(tabla.creationDate)]]></queryString>

