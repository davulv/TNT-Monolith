                  <h:panelGroup>
            <h:message styleClass="error" showSummary="true" showDetail="false" for="beginDate" />
            <t:inputCalendar id="beginDate" value="#{requestHolidayBean.beginDate}"  required="true" styleClass="requiredFieldClass"
