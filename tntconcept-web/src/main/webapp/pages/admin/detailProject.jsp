	<td class="detailFieldCell">
         	       <h:outputText value="#{projectBean.open}" />
 	      </td>
    </tr>
                            
    
  
  
      <%-- Field: name --%>
    <tr>
	<td class="detailLabelRW">${msg['project.name']}:</td>
	<td class="detailFieldCell">
