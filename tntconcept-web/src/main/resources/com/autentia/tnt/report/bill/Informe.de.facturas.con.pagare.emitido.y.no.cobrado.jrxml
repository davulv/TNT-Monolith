	<queryString><![CDATA[SELECT b.id as billId,b.number as billNumber,ct.number as creditTitleNumber,ct.amount as creditTitleAmount, ct.issueDate, ct.expirationDate, o.name
	,sum((bb.units*bb.amount)*(1+(bb.iva/100))) as billAmount
from Bill b left join BillBreakDown bb on b.id=bb.billId, CreditTitle ct, CreditTitle_Bill ctb, Organization o
where b.state='in_credittitle'
and b.id = ctb.billId
and ct.id = ctb.creditTitleId
and ct.state='emitted'
and ct.type='issued'
and bb.billId=b.id
and o.id=ct.organizationId
group by b.id
order by b.number desc]]></queryString>

