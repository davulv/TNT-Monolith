	<queryString><![CDATA[SELECT b.number,o.name,b.creationDate, max(bp.expirationDate) AS expiration
	,sum((bb.units*bb.amount)*(1+(bb.iva/100))) as sumaFactura
from Bill b left join BillBreakDown bb on b.id=bb.billId, Organization o, Project p, BillPayment bp
where b.projectId = p.id and p.organizationId = o.id and b.state='emitted' and b.billType='issued'
and bp.billId = b.id
and bb.billId=b.id
and b.creationDate between $P{Fecha inicio} and $P{Fecha fin} 
group by b.id
order by expiration desc]]></queryString>

