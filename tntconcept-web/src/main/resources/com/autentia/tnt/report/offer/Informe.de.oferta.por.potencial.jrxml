	<queryString><![CDATA[SELECT offerTitle, userName, orgName, offerPotential,offerState,validityDate,creationDate,description 
FROM ( SELECT 
case o.offerPotential when 'HIGH' then 'Alto'
                    when 'MEDIUM' then 'Medio'
                    when 'LOW' then 'Bajo'
                    end as offerPotential,
case o.offerPotential when 'HIGH' then '1'
                    when 'MEDIUM' then '2'
                    when 'LOW' then '3'
                    end as potencial,
o.description,o.creationDate,o.validityDate,
case o.offerState when 'ACCEPTED' then 'Aceptada'
                    when 'REJECTED' then 'Rechazada'
                    when 'OPEN' then 'Abierta'
                    end as offerState
,o.title as offerTitle,u.login as userName,
org.name as orgName,c.name as contactName 
	FROM Offer o, User u, Organization org, Contact c
	where org.id = o.organizationId and u.id = o.userId and c.id = o.contactId
	 and o.creationDate between $P{Fecha inicio} and $P{Fecha fin} 
order by potencial) as tabla;]]></queryString>

