	<queryString><![CDATA[SELECT o.name as company,
o.postalCode as postalCode,
o.street as street,
o.number as number,
o.city as city,
o.locator as locator,
o.state as state,
pr.name as province,
c.name as contact
from Organization o left join Province pr on pr.id = o.provinceId left join ContactInfo ci on ci.organizationId = o.id left join Contact c on ci.contactId = c.id,
OrganizationType ot
where ot.id = 1
and ot.id = o.organizationTypeId
order by o.name]]></queryString>

