	<queryString><![CDATA[SELECT o.name,sum((bb.units*bb.amount)*(1+(bb.iva/100))) AS suma
from Bill b left join BillBreakDown bb on b.id=bb.billId, Organization o, Project p
where b.projectId = p.id and p.organizationId = o.id and b.state='paid' and b.billType='issued'
and b.creationDate between $P{Fecha inicio} and $P{Fecha fin}
group by o.id]]></queryString>

