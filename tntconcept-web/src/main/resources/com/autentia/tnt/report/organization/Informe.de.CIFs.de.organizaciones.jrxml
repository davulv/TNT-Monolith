	<queryString><![CDATA[SELECT o.name as name, o.id as id, o.postalCode as postalCode, o.cif as cif, ot.name as organizationType
	from Bill b left join BillBreakDown bb on b.id=bb.billId, Organization o, OrganizationType ot
	where bb.billId=b.id
	and b.providerId = o.id and (ot.id = 1 or ot.id = 2) and ot.id = o.organizationTypeId
	and b.creationDate between $P{Fecha inicio} and $P{Fecha fin} 
	group by o.name
	union
	SELECT o.name, o.id, o.postalCode, o.cif, ot.name
	from Bill b left join BillBreakDown bb on b.id=bb.billId, Organization o, Project p, OrganizationType ot
	where bb.billId=b.id
	and p.organizationId = o.id and (ot.id = 1 or ot.id = 2) and ot.id = o.organizationTypeId
	and b.projectId = p.id and b.creationDate between $P{Fecha inicio} and $P{Fecha fin} 
	group by o.name]]></queryString>

