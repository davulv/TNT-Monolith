<%@page language="java" contentType="text/html; charset=UTF-8"%>

<%@include file="/inc/tlibs.jsp" %>

<html>
  <head>
    <%@include file="/inc/uiCore.jsp" %>
  </head>	
  <body>
    
    <!-- editProject.jsp: generated by stajanov code generator -->
    <f:loadBundle basename="com.autentia.tnt.resources.messages" var="msg" />
    <i:location place="detailProject" msg="${msg}"/> 
    
    <f:view>
      <%@include file="/inc/header.jsp" %>
      <h:form id="project" enctype="multipart/form-data">
        
        <%-- Header --%>
        <i:titleBar name="${projectBean.name}" msg="${msg}">
          <t:commandLink action="#{projectBean.edit}" immediate="true" rendered="#{projectBean.editAvailable}">
